{
  "expo": {
    "name": "Daily Journal",
    "slug": "daily-journal",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F5F5F5"
    },
    "android": {
      "package": "com.omarbinfahad.dailyjournal",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "extra": {
      "eas": {
        "projectId": "c587116a-42cd-401f-a367-9e57345e4298"
      }
    },
    "plugins": [
      "@react-native-community/datetimepicker"
    ]
  }
}
